{
  "name": "wechaty",
  "displayName": "Wechaty",
  "description": "Wechat for VSCode",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Wechat"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/imlinhanchao/wechat-cs.git"
  },
  "publisher": "hancel",
  "license": "MIT",
  "activationEvents": [],
  "icon": "assets/logo.png",
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Wechaty",
      "properties": {
        "wechaty.proxy": {
          "type": "string",
          "default": "",
          "description": "所有的请求代理"
        },
        "wechaty.websocket": {
          "type": "string",
          "default": "",
          "description": "Wechat 服务 Websocket 地址"
        },
        "wechaty.server": {
          "type": "string",
          "default": "",
          "description": "Wechat 服务地址"
        },
        "wechaty.blocks": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "屏蔽的用户列表"
        }
      }
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "wechat-webview",
          "title": "Wechat",
          "icon": "assets/logo.svg"
        }
      ]
    },
    "views": {
      "wechat-webview": [
        {
          "id": "wechaty.panel",
          "name": "Wechat",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "cd webview && npm run build && cd .. && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "dev": "cd webview && npm run dev",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.29",
    "@types/node-fetch": "^2.6.11",
    "@types/vscode": "^1.87.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "form-data": "^4.0.1",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "<3.0.0",
    "querystring": "^0.2.1",
    "reconnecting-websocket": "^4.4.0",
    "ws": "^8.18.0"
  }
}
